{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                ".next/**",
                "!.next/cache/**"
            ]
        },
        "dev": {
            "persistent": true,
            "outputs": [
                ".next/**",
                "!.next/cache/**"
            ]
        },
        "start": {
            "persistent": true
        },
        "test": {
            "outputs": [
                ".next/**",
                "!.next/cache/**"
            ]
        },
        "test:e2e": {
            "outputs": [
                ".next/**",
                "!.next/cache/**"
            ]
        },
        "lint": {}
    }
}